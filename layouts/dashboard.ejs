<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin Cinema</title>
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.1/font/bootstrap-icons.css"
      integrity="sha384-4LISF5TTJX/fLmGSxO53rV4miRxdg84mZsxmO8Rx5jGtp/LbrixFETvWa5a6sESd"
      crossorigin="anonymous" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css" />
    <link href="https://cdn.lineicons.com/4.0/lineicons.css" rel="stylesheet" />
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <div class="d-flex">
      <!-- Sidebar -->
      <aside id="sidebar" class="sidebar-toggle">
        <div class="sidebar-logo">
          <a href="/dashboard/overview">Admin Cinema</a>
        </div>
        <!-- Sidebar Navigation -->
        <ul class="sidebar-nav p-0">
          <li class="sidebar-header">Tools</li>
          <li class="sidebar-item">
            <a href="/dashboard/overview" class="sidebar-link">
              <i class="bi bi-speedometer me-2"></i>
              <span>Dashboard</span>
            </a>
          </li>
          <!-- <li class="sidebar-header">Pages</li> -->
          <li class="sidebar-item">
            <a
              href="#"
              class="sidebar-link collapsed has-dropdown"
              data-bs-toggle="collapse"
              data-bs-target="#staff"
              aria-expanded="true"
              aria-controls="auth">
              <i class="bi bi-people me-2"></i>
              <span>Quản lý nhân viên</span>
            </a>
            <ul id="staff" class="sidebar-dropdown list-unstyled collapse" data-bs-parent="#sidebar">
              <li class="sidebar-item">
                <a href="/employee/list" class="sidebar-link">Danh sách nhân viên</a>
              </li>
              <li class="sidebar-item">
                <a href="/employee/add" class="sidebar-link">Tạo nhân viên</a>
              </li>
            </ul>
          </li>
          <li class="sidebar-item">
            <a
              href="#"
              class="sidebar-link collapsed has-dropdown"
              data-bs-toggle="collapse"
              data-bs-target="#user"
              aria-expanded="true"
              aria-controls="auth">
              <i class="bi bi-person-hearts me-2"></i>
              <span>Quản lý khách hàng</span>
            </a>
            <ul id="user" class="sidebar-dropdown list-unstyled collapse" data-bs-parent="#sidebar">
              <li class="sidebar-item">
                <a href="/customer/list" class="sidebar-link">Danh sách khách hàng</a>
              </li>
            </ul>
          </li>
          <li class="sidebar-item">
            <a
              href="#"
              class="sidebar-link collapsed has-dropdown"
              data-bs-toggle="collapse"
              data-bs-target="#ticket"
              aria-expanded="true"
              aria-controls="auth">
              <i class="bi bi-bag me-2"></i>
              <span>Quản lý vé</span>
            </a>
            <ul id="ticket" class="sidebar-dropdown list-unstyled collapse" data-bs-parent="#sidebar">
              <li class="sidebar-item">
                <a href="/tickets/list" class="sidebar-link">Danh sách vé</a>
              </li>
            </ul>
          </li>
          <li class="sidebar-item">
            <a
              href="#"
              class="sidebar-link collapsed has-dropdown"
              data-bs-toggle="collapse"
              data-bs-target="#movie"
              aria-expanded="true"
              aria-controls="auth">
              <i class="bi bi-film me-2"></i>
              <span>Quản lý phim</span>
            </a>
            <ul id="movie" class="sidebar-dropdown list-unstyled collapse" data-bs-parent="#sidebar">
              <li class="sidebar-item">
                <a href="/movie/list" class="sidebar-link">Danh sách phim</a>
              </li>
              <li class="sidebar-item">
                <a href="/movie/add" class="sidebar-link">Tạo phim</a>
              </li>
              <li class="sidebar-item">
                <a href="/movie/listActor" class="sidebar-link">Danh diễn viên</a>
              </li>
              <li class="sidebar-item">
                <a href="/movie/actor" class="sidebar-link">Thêm diễn viên</a>
              </li>
              <li class="sidebar-item">
                <a href="/movie/listDirectors" class="sidebar-link">Danh sách đạo diễn</a>
              </li>
              <li class="sidebar-item">
                <a href="/movie/addDirectors" class="sidebar-link">Thêm đạo diễn</a>
              </li>
              <li class="sidebar-item">
                <a href="/genre/listGenre" class="sidebar-link">Danh thể loại</a>
              </li>
              <li class="sidebar-item">
                <a href="/genre/addGenre" class="sidebar-link">Thêm thể loại</a>
              </li>
            </ul>
          </li>
          <li class="sidebar-item">
            <a
              href="#"
              class="sidebar-link collapsed has-dropdown"
              data-bs-toggle="collapse"
              data-bs-target="#seat"
              aria-expanded="true"
              aria-controls="auth">
              <i class="bi bi-table me-2"></i>
              <span>Quản lý ghế</span>
            </a>
            <ul id="seat" class="sidebar-dropdown list-unstyled collapse" data-bs-parent="#sidebar">
              <li class="sidebar-item">
                <a href="/seat/list" class="sidebar-link">Danh sách ghế</a>
              </li>
            </ul>
          </li>
          <li class="sidebar-item">
            <a
              href="#"
              class="sidebar-link collapsed has-dropdown"
              data-bs-toggle="collapse"
              data-bs-target="#cinemaA"
              aria-expanded="true"
              aria-controls="auth">
              <i class="bi bi-camera-reels me-2"></i>
              <span>Quản lý rạp phim</span>
            </a>
            <ul id="cinemaA" class="sidebar-dropdown list-unstyled collapse" data-bs-parent="#sidebar">
              <li class="sidebar-item">
                <a href="/cinema/cinemaList" class="sidebar-link">Danh sách rạp phim</a>
              </li>
              <li class="sidebar-item">
                <a href="/cinema/cinemaAdd" class="sidebar-link">Tạo rạp phim</a>
              </li>
              <li class="sidebar-item">
                <a href="/cinema/roomList" class="sidebar-link">Danh sách phòng chiếu</a>
              </li>
              <li class="sidebar-item">
                <a href="/cinema/roomAdd" class="sidebar-link">Tạo phòng chiếu</a>
              </li>
            </ul>
          </li>
          <li class="sidebar-item">
            <a
              href="#"
              class="sidebar-link collapsed has-dropdown"
              data-bs-toggle="collapse"
              data-bs-target="#discount"
              aria-expanded="true"
              aria-controls="auth">
              <i class="bi bi-percent me-2"></i>
              <span>Quản lý khuyến mãi</span>
            </a>
            <ul id="discount" class="sidebar-dropdown list-unstyled collapse" data-bs-parent="#sidebar">
              <li class="sidebar-item">
                <a href="/discount/discountList" class="sidebar-link">Danh sách khuyến mãi</a>
              </li>
              <li class="sidebar-item">
                <a href="/discount/discountAdd" class="sidebar-link">Tạo khuyến mãi</a>
              </li>
            </ul>
          </li>
          <li class="sidebar-item">
            <a
              href="#"
              class="sidebar-link collapsed has-dropdown"
              data-bs-toggle="collapse"
              data-bs-target="#service"
              aria-expanded="true"
              aria-controls="auth">
              <i class="bi bi-cup-hot me-2"></i>
              <span>Quản lý dịch vụ</span>
            </a>
            <ul id="service" class="sidebar-dropdown list-unstyled collapse" data-bs-parent="#sidebar">
              <li class="sidebar-item">
                <a href="/service/serviceList" class="sidebar-link">Danh sách dịch vụ</a>
              </li>
              <li class="sidebar-item">
                <a href="/service/serviceAdd" class="sidebar-link">Tạo dịch vụ</a>
              </li>
            </ul>
          </li>
          <li class="sidebar-item">
            <a
              href="#"
              class="sidebar-link collapsed has-dropdown"
              data-bs-toggle="collapse"
              data-bs-target="#showtime"
              aria-expanded="true"
              aria-controls="auth">
              <i class="bi bi-calendar-date me-2"></i>
              <span>Quản lý suất chiếu</span>
            </a>
            <ul id="showtime" class="sidebar-dropdown list-unstyled collapse" data-bs-parent="#sidebar">
              <li class="sidebar-item">
                <a href="/showtime/list/showtime" class="sidebar-link">Danh sách suất chiếu</a>
              </li>
              <li class="sidebar-item">
                <a href="/showtime/add/time" class="sidebar-link">Tạo giờ chiếu</a>
              </li>
              <li class="sidebar-item">
                <a href="/showtime/add/showtime" class="sidebar-link">Tạo suất chiếu</a>
              </li>
            </ul>
          </li>
        </ul>
        <!-- Sidebar Navigation Ends -->
        <div class="sidebar-footer">
          <a href="/logout" class="sidebar-link">
            <i class="lni lni-exit"></i>
            <span class="btn btn-danger mb-2 ms-2">Đăng xuất</span>
          </a>
        </div>
      </aside>
      <!-- Sidebar Ends -->
      <!-- Main Component -->
      <div class="main">
        <nav class="navbar navbar-expand">
          <button class="toggler-btn" type="button">
            <i class="lni lni-text-align-left"></i>
          </button>
        </nav>
        <main>
          <div class="container-fluid"><%- body %></div>
        </main>
      </div>
    </div>
    <script src="/javascripts/script.js"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <!-- Alert thông báo -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
  </body>
</html>
